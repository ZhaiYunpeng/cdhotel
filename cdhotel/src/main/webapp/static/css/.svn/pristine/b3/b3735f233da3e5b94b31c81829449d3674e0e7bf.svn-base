.service-banner {
    width: 100%;
    overflow: hidden;
    position: relative;
    background: #363C50;
    z-index: 0;
    margin: 0;
}
img.service-banner-bg {
    position: absolute;
    left: 50%;
    top: 0;
    margin: 0 0 0 -840px;
    z-index: -1;
}
.profile-contents {  
    margin: 52px 0 0 20px;  
}
.service-name {
    font-size: 32px;
    color: #fff;
    line-height: 50px;
    margin: 0;
    padding: 10px 0 0 0;
   
}
.service-describe {
    font-size: 12px;
    line-height: 20px;
    color: #FBFBFB; 
    margin: 0 0 15px 0;
    padding: 0;
    width: 600px;
} 
.service-profile {
    z-index: 10;
    width: 1200px;
    height: 100%;
    overflow: hidden;
    height: 256px;
    margin: 0 auto;
}
.container{
	position: relative;
    z-index: 4;
    min-height: 500px;  
    max-width: 1200px;
    margin: 0 auto;
}
.mwrapper {
    position: relative;
    width: 100%;
    min-width: 1050px;
    margin: 20px 0 0 0;
}
.contents {
    overflow: hidden;
    margin: 0 0 0 230px;
    min-width: 800px;
    line-height: 20px; 
    word-wrap: break-word;
    position: relative;

}
.txtstyle2 {
    width: 100%;
    overflow: hidden;
    display: none;
    min-height: 400px;
    position: relative;
    padding: 15px 0 0 0;
}
.bluetitle {
    width: 100%;
    height: 40px;
    position: relative;
    line-height: 40px;
    font-size: 16px;
    color: #333;
    margin: 10px 0 10px 0;
}
.serve-explain-text {
    overflow: hidden;
    margin: 0 0 30px 0;
    padding: 0 0 0 20px;
    width: 930px;
    color: #666;
    line-height: 20px;
}

.serveintor-min {
    width: 100%;
    overflow: hidden;
} 

.serveintortxt {
    position: relative;
    overflow: hidden;
    padding: 0 0 10px 0;
    margin: 0 0 30px 20px;
    border-bottom: 1px dashed #f5f5f5;
}
table{ 
table-layout:fixed; 
empty-cells:show; 
border-collapse: collapse; 
margin:0 auto; 
} 
.tableMap{  
border:1px solid #cad9ea; 
color:#666; 
} 
.tableMap th { 
background-repeat:repeat-x; 
  
} 
.tableMap td,.tableMap th{  
border:1px solid #cad9ea; 
padding:0 1em 0;  
height:40px; 
}
#dead_item { 
	background: #AAAAAA          
}
 